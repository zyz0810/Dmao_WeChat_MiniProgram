<template name="toast">
  <view class='toast_content_box' wx:if="{{isHide}}">
    <view class='toast_content tc'>
      <view class='toast_icon'>
        <!-- <image src='{{icon}}'></image> -->
        <text class='iconfont {{icon}} f40'></text>
      </view>
      <view class='toast_content_text f14'>
        {{content}}
      </view>
    </view>
  </view>
</template>